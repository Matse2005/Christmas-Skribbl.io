{
  "manifest_version": 2,
  "name": "Christmas Skribble.io",
  "version": "0.0.1-beta",
  "description": "Christmasify your skribble.io",
  "icons": {
    "48": "logo/logo_48x48.png",
    "512": "logo/logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.skribble.io/*"
      ],
      "js": [
        "scripts/run.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "Christmas-Skribble.io@matsevh"
    }
  }
}